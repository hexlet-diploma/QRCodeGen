{
  "manifest_version": 3,
  "name": "QR Code Generator",
  "description": "",
  "version": "1.0",
  "icons": {
    "128": "src/assets/icons/icon.png"
  },
  "action": {
    "default_popup": "src/index.html",
    "default_title": "QR Code"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "activeTab",
    "storage",
    "downloads"
  ]
}
